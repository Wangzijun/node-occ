{
  "name": "node-occ",
  "version": "0.2.2",
  "description": "OpenCascade OCCT Wrapper for Node js",
  "preferGlobal": "true",
  "dependencies": {
    "async": "^2.5.0",
    "colors": "*",
    "gettemporaryfilepath": "1.0.0",
    "nan": "^2.6.2",
    "node-pre-gyp": "^0.6.36",
    "pace": "~0.0.4",
    "progress": "~2.0.0"
  },
  "devDependencies": {
    "aws-sdk": "2.x",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-require-path-exists": "^1.1.7",
    "eslint-plugin-standard": "^3.0.1",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-nodeunit": "~1.0.0",
    "grunt-contrib-uglify": "3.0.1",
    "grunt-docco": "~0.5.0",
    "grunt-mocha-cli": "~3.0.0",
    "mocha": "~3.5.0",
    "should": "~11.2.1"
  },
  "main": "index",
  "author": {
    "name": "Etienne Rossignon"
  },
  "engines": {
    "node": "*"
  },
  "keywords": [
    "3D",
    "CAD",
    "OpenCascade",
    "OCE",
    "Solid Modeler",
    "geometry",
    "BRep"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebcad/node-occ.git"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha -R spec",
    "lint": "eslint lib test bin",
    "doc": "grunt doc"
  },
  "binary": {
    "module_name": "occ",
    "module_path": "./lib/binding/",
    "host-aws": "https://nodeocc.s3-us-west-1.amazonaws.com",
    "host": "https://github.com/openwebcad/node-occ/releases/download/",
    "remote_path": "{version}"
  },
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/openwebcad/node-occ/issues"
  },
  "directories": {
    "test": "test"
  },
  "license": "MIT"
}
